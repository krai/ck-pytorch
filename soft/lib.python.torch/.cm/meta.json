{
  "auto_detect": "yes",
  "customize": {
    "check_that_exists": "yes",
    "env_prefix": "CK_ENV_PYTHON_PACKAGE_TORCH",
    "limit_recursion_dir_search": {
      "linux": 5,
      "win": 5
    },
    "soft_file_universal": "torch$#sep#$__init__.py",
    "version_module_name": "version",
    "version_variable_name": "__version__"
  },
  "deps": {
    "python": {
      "local": "yes",
      "name": "Python interpreter",
      "sort": 10,
      "tags": "compiler,lang-python"
    }
  },
  "soft_name": "Python Torch library",
  "tags": [
    "lib",
    "python-package",
    "pytorch",
    "torch"
  ]
}
